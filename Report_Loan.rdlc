      <Query>
        <DataSourceName>synod_dbConnectionString</DataSourceName>
        <CommandText>SELECT        installment.acc_no, installment.name, installment.pmt_no, installment.pmt_date, installment.bb, installment.sp, installment.ep, installment.tp, installment.ir, 
                         installment.eb, installment.rem, loan.loan_amt, loan.air, loan.period, loan.p_type, loan.nppy, loan.start, loan.snp, loan.sp AS Expr1
FROM            installment INNER JOIN
                         loan ON installment.acc_no = loan.acc_no
WHERE        (installment.acc_no = @acc_no)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
